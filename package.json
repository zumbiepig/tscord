{
	"name": "tscord",
	"version": "2.6",
	"description": "A fully-featured discord bot template written in Typescript, intended to provide a framework that's easy to use, extend and modify",
	"license": "MIT",
	"repository": "github:barthofu/tscord",
	"type": "module",
	"module": "src/main.ts",
	"engines": {
		"node": ">=20"
	},
	"engineStrict": true,
	"scripts": {
		"postinstall": "bun run -c installoop --rootDir=./src/plugins",
		"start": "NODE_ENV=production bun run -c ./dist/main.js",
		"dev": "NODE_ENV=development bun run -c --hot .",
		"build": "rm -rf ./dist && bun build --minify --sourcemap --target=bun --outdir=dist ./src/main.ts",
		"build:exec": "rm -rf ./dist && bun build --compile --minify --sourcemap --outfile=dist/tscord-bot ./src/main.ts",
		"lint": "bun run -c eslint .",
		"lint:fix": "bun run -c eslint --fix .",
		"format": "bun run -c prettier --write --list-different .",
		"format:check": "bun run -c prettier --check .",
		"i18n": "bun run -c typesafe-i18n",
		"migration:create": "bun run -c mikro-orm migration:create",
		"migration:up": "bun run -c mikro-orm migration:up",
		"migration:down": "bun run -c mikro-orm migration:down"
	},
	"dependencies": {
		"@discordx/importer": "~1.3.1",
		"@discordx/pagination": "~3.5.5",
		"@discordx/utilities": "~7.0.3",
		"@mikro-orm/core": "~6.4.3",
		"@mikro-orm/entity-generator": "~6.4.3",
		"@mikro-orm/migrations": "~6.4.3",
		"@mikro-orm/sql-highlighter": "~1.0.1",
		"@mikro-orm/sqlite": "~6.4.3",
		"@tsed/common": "^8.4.2",
		"@tsed/exceptions": "^8.4.2",
		"@tsed/platform-express": "^8.4.2",
		"@tsed/schema": "^8.4.2",
		"@tsed/swagger": "^8.4.2",
		"archiver": "^7.0.1",
		"axios": "~1.7.9",
		"body-parser": "^1.20.3",
		"boxen": "~8.0.1",
		"case": "~1.6.3",
		"chalk": "~5.4.1",
		"chokidar": "~4.0.3",
		"core-js": "^3.40.0",
		"cron": "~3.5.0",
		"cron-validator": "~1.3.1",
		"dayjs": "~1.11.13",
		"discord-api-types": "~0.37.115",
		"discord-logs": "~2.2.1",
		"discord-oauth2": "~2.12.1",
		"discord.js": "~14.17.3",
		"discordx": "~11.12.2",
		"envalid": "^8.0.0",
		"fast-folder-size": "~2.3.0",
		"image-hash": "~5.3.2",
		"imgur": "~2.4.2",
		"node-os-utils": "~1.3.7",
		"ora": "~8.1.1",
		"pidusage": "~3.0.2",
		"rentry-pastebin": "~1.3.0",
		"rxeta": "~1.1.2",
		"saveqlite": "~1.1.2",
		"semver": "^7.6.3",
		"stacktrace-parser": "~0.1.10",
		"ts-toolbelt": "^9.6.0",
		"tsyringe": "~4.8.0",
		"typesafe-i18n": "^5.26.2"
	},
	"devDependencies": {
		"@mikro-orm/cli": "^6.4.3",
		"@tsconfig/bun": "^1.0.7",
		"@tsconfig/strictest": "^2.0.5",
		"@types/archiver": "^6.0.3",
		"@types/bun": "^1.1.16",
		"@types/node-os-utils": "^1.3.4",
		"@types/pidusage": "^2.0.5",
		"@types/semver": "^7.5.8",
		"bun": "^1.1.43",
		"eslint": "^9.18.0",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"installoop": "^1.1.0",
		"prettier": "^3.4.2",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.20.0"
	},
	"trustedDependencies": [
		"bun",
		"core-js"
	],
	"tscord": {
		"version": "2.6"
	},
	"mikro-orm": {
		"useTsNode": true
	}
}
