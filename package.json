{
  "name": "tscord",
  "version": "2.6.0",
  "description": "A fully-featured discord bot template written in Typescript, intended to provide a framework that's easy to use, extend and modify",
  "license": "MIT",
  "repository": "github:barthofu/tscord",
  "type": "module",
  "main": "dist/main.js",
  "module": "src/main.ts",
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "engineStrict": true,
  "scripts": {
    "postinstall": "installoop --rootDir=./src/plugins/* && npm run i18n",
    "start": "cross-env NODE_ENV=production node .",
    "dev": "npm-run-all --parallel i18n:watch dev:dev",
    "dev:dev": "cross-env NODE_ENV=development tsx watch src/main.ts",
    "build": "rimraf ./dist && npm run i18n && tsc && tsc-alias",
    "lint": "npm-run-all --parallel lint:lint typecheck",
    "lint:lint": "eslint .",
    "lint:fix": "npm-run-all --parallel 'lint:lint --fix' typecheck",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write --list-different .",
    "format:check": "prettier --check .",
    "i18n": "typesafe-i18n --no-watch",
    "i18n:watch": "typesafe-i18n"
  },
  "dependencies": {
    "@discordx/pagination": "^3.5.5",
    "@discordx/utilities": "^7.0.3",
    "@mikro-orm/better-sqlite": "^6.4.5",
    "@mikro-orm/core": "^6.4.3",
    "@mikro-orm/entity-generator": "^6.4.5",
    "@mikro-orm/migrations": "^6.4.5",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@tsed/common": "^8.4.5",
    "@tsed/exceptions": "^8.4.4",
    "@tsed/platform-express": "^8.4.5",
    "@tsed/schema": "^8.4.7",
    "@tsed/swagger": "^8.4.5",
    "body-parser": "^1.20.3",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "change-case": "^5.4.4",
    "chokidar": "^4.0.3",
    "cron": "^3.5.0",
    "cron-validator": "^1.3.1",
    "dayjs": "^1.11.13",
    "discord-logs": "^2.2.1",
    "discord-oauth2": "^2.12.1",
    "discord.js": "^14.17.3",
    "discordx": "^11.12.2",
    "dotenv": "^16.4.7",
    "envalid": "^8.0.0",
    "glob": "^11.0.1",
    "imgur": "^2.4.2",
    "node-os-utils": "^1.3.7",
    "ora": "^8.2.0",
    "pidusage": "^3.0.2",
    "reflect-metadata": "^0.2.2",
    "rentry-pastebin": "^1.3.0",
    "rxeta": "^1.1.2",
    "semver": "^7.7.1",
    "stacktrace-parser": "^0.1.10",
    "tar": "^7.4.3",
    "tsyringe": "^4.8.0",
    "typesafe-i18n": "^5.26.2"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^6.4.5",
    "@tsconfig/node-lts": "^22.0.1",
    "@tsconfig/strictest": "^2.0.5",
    "@types/archiver": "^6.0.3",
    "@types/node": "^22.13.1",
    "@types/node-os-utils": "^1.3.4",
    "@types/pidusage": "^2.0.5",
    "@types/semver": "^7.5.8",
    "cross-env": "^7.0.3",
    "eslint": "^9.19.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "installoop": "^1.1.0",
    "jiti": "^2.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "type-fest": "^4.33.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0"
  },
  "tscord": {
    "version": "2.6"
  }
}
