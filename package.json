{
	"name": "tscord",
	"version": "2.6.0",
	"description": "A fully-featured discord bot template written in Typescript, intended to provide a framework that's easy to use, extend and modify",
	"license": "MIT",
	"repository": "github:barthofu/tscord",
	"type": "module",
	"main": "dist/main.js",
	"module": "src/main.ts",
	"engines": {
		"node": ">=22",
		"npm": ">=10"
	},
	"engineStrict": true,
	"packageManager": "npm@11.1.0",
	"scripts": {
		"postinstall": "installoop --rootDir=./src/plugins/* && npm run i18n",
		"start": "cross-env NODE_ENV=production node .",
		"dev": "npm-run-all --parallel i18n:watch dev:dev",
		"dev:dev": "cross-env NODE_ENV=development ts-node src/main.ts",
		"build": "rimraf ./dist && npm run i18n && tsc && tsc-alias",
		"lint": "npm run typecheck && npm run lint:lint",
		"lint:lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"typecheck": "tsc --noEmit",
		"format": "prettier --write --list-different .",
		"format:check": "prettier --check .",
		"i18n": "typesafe-i18n --no-watch",
		"i18n:watch": "typesafe-i18n"
	},
	"dependencies": {
		"@discordx/pagination": "^3.5.6",
		"@discordx/utilities": "^7.0.3",
		"@mikro-orm/better-sqlite": "^6.4.7",
		"@mikro-orm/core": "^6.4.7",
		"@mikro-orm/entity-generator": "^6.4.9",
		"@mikro-orm/mariadb": "^6.4.7",
		"@mikro-orm/migrations": "^6.4.7",
		"@mikro-orm/mongodb": "^6.4.7",
		"@mikro-orm/postgresql": "^6.4.7",
		"@mikro-orm/reflection": "^6.4.7",
		"@mikro-orm/sql-highlighter": "^1.0.1",
		"@tsed/common": "^8.5.2",
		"@tsed/exceptions": "^8.5.2",
		"@tsed/platform-express": "^8.5.2",
		"@tsed/schema": "^8.5.2",
		"@tsed/swagger": "^8.5.2",
		"archiver": "^7.0.1",
		"boxen": "^8.0.1",
		"chalk": "^5.4.1",
		"change-case": "^5.4.4",
		"chartjs-to-image": "^1.2.2",
		"chokidar": "^4.0.3",
		"cron": "^4.1.0",
		"cron-validator": "^1.3.1",
		"dayjs": "^1.11.13",
		"discord-oauth2": "^2.12.1",
		"discord.js": "^14.18.0",
		"discordx": "^11.12.2",
		"dotenv": "^16.4.7",
		"envalid": "^8.0.0",
		"glob": "^11.0.1",
		"imgur": "^2.4.3",
		"ora": "^8.2.0",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.2",
		"semver": "^7.7.1",
		"tsyringe": "^4.8.0",
		"typesafe-i18n": "^5.26.2"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.7",
		"@eslint/js": "^9.21.0",
		"@stylistic/eslint-plugin": "^4.2.0",
		"@tsconfig/node22": "^22.0.0",
		"@tsconfig/strictest": "^2.0.5",
		"@types/archiver": "^6.0.3",
		"@types/node": "^22.13.9",
		"@types/semver": "^7.5.8",
		"cross-env": "^7.0.3",
		"eslint": "^9.21.0",
		"eslint-plugin-n": "^17.16.1",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-unicorn": "^57.0.0",
		"installoop": "^1.1.0",
		"jiti": "^2.4.2",
		"nodemon": "^3.1.9",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.5.3",
		"rimraf": "^6.0.1",
		"ts-node": "^10.9.2",
		"tsc-alias": "^1.8.11",
		"type-fest": "^4.36.0",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.0"
	},
	"tscord": {
		"version": "2.6"
	}
}
